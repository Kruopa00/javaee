<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
        PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
        "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">
<generatorConfiguration>
    <context id="lab01" targetRuntime="MyBatis3Simple">
        <jdbcConnection driverClass="org.h2.Driver"
                        connectionURL="jdbc:h2:~/h2database/Lab01DB;AUTO_SERVER=TRUE"
                        userId="sa"
                        password="sa">
        </jdbcConnection>

        <javaModelGenerator targetPackage="org.example.lab01.mybatis.model" targetProject="MAVEN" />

        <sqlMapGenerator targetPackage="org.example.lab01.mybatis.xml" targetProject="MAVEN" />
        <javaClientGenerator type="XMLMAPPER" targetPackage="org.example.lab01.mybatis.dao" targetProject="MAVEN" />


        <table schema="public" tableName="studentas">
            <generatedKey column="id" identity="true" sqlStatement="JDBC" />
        </table>
        <table schema="public" tableName="grupe">
            <generatedKey column="id" identity="true" sqlStatement="JDBC" />
            <ignoreColumn column="opt_lock_version" />
        </table>
        <table schema="public" tableName="pasirenkamaskursas">
            <generatedKey column="id" identity="true" sqlStatement="JDBC" />
        </table>
    </context>
</generatorConfiguration>